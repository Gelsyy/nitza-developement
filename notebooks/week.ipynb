   "source": [
    "from datetime import datetime, timedelta\n",
    "\n",
    "day = '12/Oct/2013'\n",
    "dt = datetime.strptime(day, '%d/%b/%Y')\n",
    "start = dt - timedelta(days=dt.weekday())\n",
    "end = start + timedelta(days=6)\n",
    "\n",
    "print(start)\n",
    "print(end)"
   ]
  }
